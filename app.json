{
  "expo": {
    "name": "share-bugs",
    "slug": "share-bugs",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "sharebugs",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "image": "./assets/splash-icon.png"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.umutimamoglu.share-bugs"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.umutimamoglu.sharebugs",
      "googleServicesFile": "./google-services.json",
      "versionCode": 2
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification_icon.png",
          "color": "#cff",
          "defaultChannel": "default",
          "enableBackgroundRemoteNotifications": false
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "077998cb-e571-479a-9181-2fb9e43c4a5a"
      }
    },
    "owner": "umutimamoglu",
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/077998cb-e571-479a-9181-2fb9e43c4a5a"
    }
  }
}
